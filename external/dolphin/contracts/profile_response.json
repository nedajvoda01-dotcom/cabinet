{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DolphinProfileResponse",
  "type": "object",
  "required": ["correlation_id", "status", "profile"],
  "properties": {
    "correlation_id": { "type": "string" },
    "status": { "type": "string", "enum": ["ok", "error"] },
    "profile": {
      "type": "object",
      "required": ["id", "name", "status"],
      "properties": {
        "id": { "type": ["string", "integer"] },
        "name": { "type": "string" },
        "status": { "type": "string", "enum": ["ready", "busy", "blocked"] }
      },
      "additionalProperties": true
    },
    "error": { "$ref": "./errors.json#/definitions/DolphinError" }
  },
  "additionalProperties": false
}
