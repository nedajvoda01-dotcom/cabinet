{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ParserParseResponse",
  "type": "object",
  "required": ["external_id", "status", "data", "photos", "correlation_id"],
  "properties": {
    "external_id": { "type": "string" },
    "status": { "type": "string", "enum": ["ok", "error"] },
    "correlation_id": { "type": "string" },
    "data": {
      "type": "object",
      "required": ["vehicle", "price", "location", "title", "description"],
      "properties": {
        "title": { "type": "string" },
        "description": { "type": "string" },
        "vehicle": { "type": "object", "additionalProperties": true },
        "price": { "type": "object", "additionalProperties": true },
        "location": { "type": "object", "additionalProperties": true },
        "meta": { "type": "object", "additionalProperties": true }
      },
      "additionalProperties": true
    },
    "photos": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["url", "order_no"],
        "properties": {
          "url": { "type": "string", "format": "uri" },
          "order_no": { "type": "integer", "minimum": 0 },
          "hash": { "type": "string" }
        },
        "additionalProperties": true
      }
    },
    "error": { "$ref": "./errors.json#/definitions/ParserError" }
  },
  "additionalProperties": false
}
