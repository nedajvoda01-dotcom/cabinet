{
  "version": "1.0.0",
  "platform": {
    "name": "cabinet-platform",
    "version": "1.0.0",
    "artifacts": [
      {
        "name": "platform-core",
        "type": "php-application",
        "path": "platform/",
        "entrypoints": [
          "public/index.php",
          "bin/worker.php"
        ]
      }
    ],
    "dependencies": {
      "php": "^8.1",
      "contracts": "1.0.0"
    }
  },
  "ui": {
    "desktop": {
      "name": "cabinet-desktop-ui",
      "version": "1.0.0",
      "type": "vite-application",
      "path": "ui/desktop/",
      "build_output": "ui/desktop/dist/"
    }
  },
  "adapters": [
    {
      "name": "parser-adapter",
      "version": "1.0.0",
      "path": "adapters/parser/",
      "endpoints": {
        "invoke": "/invoke",
        "descriptor": "/descriptor",
        "health": "/health"
      }
    },
    {
      "name": "photos-adapter",
      "version": "1.0.0",
      "path": "adapters/photos/",
      "endpoints": {
        "invoke": "/invoke",
        "descriptor": "/descriptor",
        "health": "/health"
      }
    },
    {
      "name": "robot-adapter",
      "version": "1.0.0",
      "path": "adapters/robot/",
      "endpoints": {
        "invoke": "/invoke",
        "descriptor": "/descriptor",
        "health": "/health"
      }
    },
    {
      "name": "storage-adapter",
      "version": "1.0.0",
      "path": "adapters/storage/",
      "endpoints": {
        "invoke": "/invoke",
        "descriptor": "/descriptor",
        "health": "/health"
      }
    },
    {
      "name": "browser-context-adapter",
      "version": "1.0.0",
      "path": "adapters/browser-context/",
      "endpoints": {
        "invoke": "/invoke",
        "descriptor": "/descriptor",
        "health": "/health"
      }
    }
  ],
  "contracts": {
    "version": "1.0.0",
    "path": "shared/contracts/",
    "compatibility": "N/N-1"
  }
}
