{
  "name": "cabinet/backend",
  "description": "Backend conveyor platform",
  "type": "project",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "Backend\\": "backend/src/",
      "Modules\\": "backend/src/Modules/",
      "App\\": "backend/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test:unit:back": "vendor/bin/phpunit --testsuite unit"
  }
}
